<!-- Example configuration file for NARS-console -->
<ROMcollections>

<!-- Nintendo ROMS -->
<collection name="Genesis with No-Intro DAT" shortname="genesis">
  <NoIntroDat>Sega - Mega Drive - Genesis Parent-Clone (20170318-044444).dat</NoIntroDat>
  <Options>NoBIOS</Options>
  <filterUpTags>Europe, World, Rev 1</filterUpTags>
  <filterDownTags>Japan, Beta, Proto, France</filterDownTags>
  <includeTags>Europe, USA</includeTags>
  <excludeTags>Japan</excludeTags>
  <!-- Search strings, separated by commas.
       ROMs names at searched after filtering.
       If substring found in ROM name then ROM will be excluded -->
  <excludeGame></excludeGame>

  <!--
  <Source     >/home/kodi/roms/roms-nintendo-snes/</Source>
  <Destination>/home/kodi/NUC-remote/ROMs/roms-nintendo-snes/</Destination>
   -->
  <SourceROMs     >./source/</SourceROMs>
  <DestinationROMs>./dest/</DestinationROMs>

  <SourceTitles    >./artwork-source/titles/</SourceTitles>
  <SourceSnaps     >./artwork-source/snaps/</SourceSnaps>
  <SourceFanarts   ></SourceFanarts>
  <SourceBanners   >./artwork-source/banners/</SourceBanners>
  <SourceClearlogos>./artwork-source/clearlogos/</SourceClearlogos>
  <SourceBoxfronts >./artwork-source/boxfronts/</SourceBoxfronts>
  <SourceBoxbacks  >./artwork-source/boxbacks/</SourceBoxbacks>
  <SourceCartridges>./artwork-source/cartridges/</SourceCartridges>
  <SourceFlyers    ></SourceFlyers>
  <SourceMaps      ></SourceMaps>
  <SourceManuals   ></SourceManuals>
  <SourceTrailers  ></SourceTrailers>

  <DestinationTitles    >./artwork-dest/titles/</DestinationTitles>
  <DestinationSnaps     >./artwork-dest/snaps/</DestinationSnaps>
  <DestinationFanarts   ></DestinationFanarts>
  <DestinationBanners   >./artwork-dest/banners/</DestinationBanners>
  <DestinationClearlogos>./artwork-dest/clearlogos/</DestinationClearlogos>
  <DestinationBoxfronts >./artwork-dest/boxfront/</DestinationBoxfronts>
  <DestinationBoxbacks  >./artwork-dest/boxback/</DestinationBoxbacks>
  <DestinationCartridges>./artwork-dest/cartridges/</DestinationCartridges>
  <DestinationFlyers    ></DestinationFlyers>
  <DestinationMaps      ></DestinationMaps>
  <DestinationManuals   ></DestinationManuals>
  <DestinationTrailers  ></DestinationTrailers>
</collection>

</ROMcollections>
